{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": [
      "dist/"
    ],
    "exclude": [
      "src/libs"
    ],
    "includePattern": "\\.js$",
    "excludePattern": "\\.d.ts"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
