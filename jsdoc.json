{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": ["src/"]
    },
    "includePattern": "\\.ts$",
    "exclude": ["src/libs"],
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "template": "node_modules/docdash",
        "encoding": "utf8",
        "destination": "./docs/",
        "recurse": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}