{
  "name": "@biulight/craco-config-extensions",
  "version": "1.0.4",
  "description": "简化craco配置，支持动态加载打包产物",
  "keywords": [
    "craco",
    "webpack",
    "html-webpack-plugin"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.esm.js",
  "type": "module",
  "homepage": "https://biulight.github.io/craco-config-extensions/index.html",
  "bugs": "https://github.com/biulight/craco-config-extensions/issues",
  "repository": "git@biulight.github.com:biulight/craco-config-extensions.git",
  "author": "biulight <biulight007@gmail.com>",
  "files": [
    "dist",
    "README.md",
    "package.json",
    "LICENSE"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "rollup --config rollup.config.js -w",
    "build": "cross-env NODE_ENV=production rollup -c",
    "build:doc": "jsdoc -c jsdoc.json",
    "test": "cross-env NODE_ENV=test && jest -o --collectCoverage",
    "test:all": "cross-env NODE_ENV=test && jest --collectCoverage",
    "eslint": "eslint --fix --ext .ts src/ __test__/",
    "clean": "rimraf dist",
    "prepare": "husky install",
    "prebuild": "npm run clean && npm run test:all",
    "prepublishOnly": "npm run build",
    "deploy": "npm publish --access public"
  },
  "lint-staged": {
    "./{src,__test__}/**/*.ts|./*.{json,md}": [
      "prettier --write"
    ],
    "./{src,__test__}/**/*.{ts,tsx}": "eslint --fix"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@rollup/plugin-alias": "^4.0.2",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/jest": "^29.2.5",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "babel-plugin-transform-import-meta": "^2.2.0",
    "core-js": "^3.31.1",
    "cross-env": "^7.0.3",
    "docdash": "^2.0.1",
    "eslint": "^8.0.1",
    "eslint-config-semistandard": "latest",
    "eslint-config-standard": "latest",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "jsdoc": "^4.0.2",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "rimraf": "^4.0.7",
    "rollup": "^3.10.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0"
  },
  "dependencies": {
    "@craco/craco": "^7.1.0",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.2.3",
    "dotenv": "^16.0.3",
    "html-webpack-plugin": "^5.5.0",
    "lodash": "^4.17.21"
  },
  "packageManager": "yarn@3.5.1"
}
